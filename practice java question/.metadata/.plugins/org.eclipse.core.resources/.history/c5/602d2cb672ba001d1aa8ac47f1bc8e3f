package operator;

public class SwitchCase3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int rating=3;
switch(rating)
{
case 1:
case 2:
	System.out.println("bad review");
	break;
case 3:
	System.out.println("average  review");
case 4:
case 5:
	System.out.println("good review");
}
	}

}
