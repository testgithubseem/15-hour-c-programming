package practice;
import java.util.Scanner;
public class SumOfTwoNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int result=a+b;
		System.out.println("result is" +result);
	

	}

}
