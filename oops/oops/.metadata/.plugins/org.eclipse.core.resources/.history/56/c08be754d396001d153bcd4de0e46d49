package oops;
class Cat{
	boolean hasfur;
	String color;
	int eyes,legs;
	public void walk()
	{
		System.out.println("cat is walking");
	}
	public void eat()
	{
		System.out.println("cat is eatking");	
	}
	public void Description()
	{
		System.out.println("my cat has"  +legs+  "legs and"  +eyes+   "eyes");
	}
	
	
}
class Dog{
	String breed,name;
	public void jump() {
		
	
	System.out.println("my dog "+name+"jumped");
	}
	public void description()
	{
		System.out.println("my dog name is" +name+ "its breed is" +breed+ "brred");
	}
	
	

public class MainClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Cat cat1=new Cat();
		Cat cat2=new Cat();
		Dog dog1=new Dog();
		Dog dog2=new Dog();
		dog1.name="browny";
		dog1.breed="huskey";
				dog2.name="mr.mulchand";
				dog2.breed="poodle";
cat1.walk();
cat2.eat();
cat1.legs=3;
cat1.eyes=2;
cat2.legs=4;
cat2.eyes=1;


cat1.Description();
cat2.Description();
dog1.jump();
dog1.description();
dog2.jump();
dog2.description();
	}

}
}
