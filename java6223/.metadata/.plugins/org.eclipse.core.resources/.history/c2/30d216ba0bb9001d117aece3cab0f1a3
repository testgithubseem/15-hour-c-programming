package java4;
import java.util.Scanner;
public class OddAndEven {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
System.out.println("enter the number");
int n=sc.nextInt();
for(int i=1;i<=n;i++)
{
	System.out.print(i);
	if(i%2==0)
	{
		System.out.print(i);
		 if(i%2==1)
		{
			System.out.print(i);
		}
		
	}
	
}


	
}
	}


